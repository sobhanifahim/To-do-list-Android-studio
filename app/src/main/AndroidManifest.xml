<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.splashscreen">

    <uses-permission android:name="android.permission.ACTIVITY_RECOGNITION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Leg_workout"></activity>
        <activity android:name=".Chest_workout" />
        <activity android:name=".Arm_workout" />
        <activity android:name=".BmiCalculator" />
        <activity android:name=".stopWatch" />
        <activity android:name=".footStep" />
        <activity android:name=".setReminder" />

        <receiver android:name=".AlarmReceiver" />

        <activity android:name=".calculateBills" />
        <activity android:name=".ExerciseList" />
        <activity android:name=".tdlist" />
        <activity android:name=".Dashboard" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>